package "cptclip"
purpose "Create cpt file with restricted z-range"
option	"output"    o "output filename"		      string no
option  "range"     R "z-range of output"             string yes
option  "segments"  s "min/max are segment numbers"   flag   off
option	"verbose"   v "verbose operation"             flag   off



