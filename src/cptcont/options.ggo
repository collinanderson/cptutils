package "cptcont"
purpose "Create a continuous colour table from non-continuous"
option  "midpoint"  m "split segments at the midpoint" flag off
option	"output"    o "output filename"		       string no
option  "partial"   p "percentage towards continuous"  double default="100.0" no
option	"verbose"   v "verbose operation"              flag   off



