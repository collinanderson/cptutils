package "pspsvg"
purpose "convert PaintShop Pro gradients to SVG gradient"
option	"output"	   o "output filename"			string  no
option  "preview"          p "include preview in output"	flag    off
option  "preview-geometry" - "geometry of preview"		string  default="250x30" no
option	"verbose"	   v "verbose operation"		flag    off




