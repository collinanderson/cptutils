package "svgcpt"
purpose "convert SVD gradient to GMT colour palette table"
option	"list"	        l "list gradients in file"           flag    off
option	"nth"	        n "select nth gradient"	             int     no
option	"output"	o "output filename"	             string  no
option	"verbose"	v "verbose operation"	             flag    off





