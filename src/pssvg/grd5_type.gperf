#include <string.h>
#include "grd5_type.h"

%}

%%
"patt", TYPE_PATTERN 
"desc", TYPE_DESCRIPTIN
"VlLs", TYPE_VlLs
"TEXT", TYPE_TEXT
"Objc", TYPE_OBJECT
"UntF", TYPE_UntF
"bool", TYPE_BOOL
"long", TYPE_LONG
"doub", TYPE_DOUBLE
%%

extern int grd5_type(char* s)
{
  if (!s) return TYPE_UNKNOWN;
  return grd5_type_lookup(s, 4);
}
