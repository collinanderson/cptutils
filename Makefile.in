# Makefile.in for cptutils

# $Id: Makefile.in,v 1.7 2010/04/19 23:37:12 jjg Exp jjg $
# help older versions make

@SET_MAKE@

# Macros

VERSION   = @PACKAGE_VERSION@
RUBBISH   = *~
CONFFILES = config.cache config.log config.status configure Makefile

DIST      = 

# targets

default : all

all : 
	cd src ; $(MAKE) all ; cd ..

install : all
	cd src ; $(MAKE) install ; cd ..

check test show : all
	cd src ; $(MAKE) test ; cd ..

clean :
	$(RM) $(RUBBISH)
	cd src ; $(MAKE) clean ; cd ..

veryclean spotless : clean version
	$(RM) $(CONFFILES)
	autoconf
	rm -rf autom4te.cache
	cd src ; $(MAKE) veryclean ; cd ..

version :
	echo $(VERSION) > VERSION




